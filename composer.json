{
    "name": "omitobisam/openid-server",
    "description": "OpenID Connect server for PHP",
    "type": "library",
    "config": {
        "preferred-install": {
            "league/oauth2-server": "source",
            "*": "dist"
        }
    },
    "require": {
        "league/oauth2-server": "^8",
        "lcobucci/jwt": "^4.3|^5.0",
        "nyholm/psr7": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.13",
        "laminas/laminas-diactoros": "^3.3.0"
    },
    "autoload": {
        "psr-4": {
            "Idaas\\OpenID\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IdaasTests\\": "tests/",
            "LeagueTests\\": "vendor/league/oauth2-server/tests/"
        }
    },
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "Oluwatobi Omisakin",
            "email": "omisakin.oluwatobi@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
